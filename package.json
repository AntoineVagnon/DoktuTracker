  "scripts": {
    "dev": "set NODE_ENV=development && tsx server/index.ts",
    "dev:frontend": "vite",
    "build": "npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/index.js --external:./vite",
    "build:full": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "start:clean": "NODE_ENV=production npm --omit=dev run start",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "process-translations": "node scripts/process-translations.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:mobile": "playwright test --project=webkit-mobile",
    "test:e2e:desktop": "playwright test --project=chromium-desktop",
    "test:e2e:tablet": "playwright test --project=chromium-tablet",
    "test:medical": "playwright test --grep=\"medical|GDPR|compliance|médical\"",
    "test:critical": "playwright test --grep=\"authentication|booking|admin\"",
    "test:integrations": "playwright test e2e/tests/05-integrations.spec.ts",
    "qa:full": "npm run test:e2e && npm run test:e2e:report",
    "qa:mobile": "npm run test:e2e:mobile && npm run test:e2e:report",
    "qa:security": "playwright test --grep=\"security|sécurité|GDPR|conformité\""
  },
